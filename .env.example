# doitsukani Environment Variables Template
# Copy this file to .env and fill in your actual API keys

VITE_APP_BASE_URL=/

# DeepL API Configuration
# Get your API key from: https://www.deepl.com/pro-api
DEEPL_API_KEY=your-deepl-api-key-here
DEEPL_PRO=false

# Wanikani API Configuration (for integration tests)
# Get your API token from: https://www.wanikani.com/settings/personal_access_tokens
# 
# SAFE TEST RADICALS for write operations: "Rice", "Spikes", "Umbrella"
# These radicals can be safely manipulated without affecting learning progress
WANIKANI_API_TOKEN=your-wanikani-api-token-here
